MODULE user_command_0100 INPUT.
  CASE sy-ucomm.
    WHEN 'EXIT'.
      LEAVE PROGRAM.
    WHEN 'BAKC'.
      LEAVE TO SCREEN 0.
    WHEN 'CANC'.
      SET SCREEN 0.
  ENDCASE.
ENDMODULE.
